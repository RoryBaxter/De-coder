LF=[]
LFT=[]
LFT2=[]
Winning=[]
WinningScore=0
Current=[]


#Counts the number of times a vaule is in an other vaule
def LetterCount(l,t):
    a=0
    for i in t:
        if i==l:
            a+=1
    return(a)

#Removes everyhting that I don't want from the text
def clean(t):
    T=""
    t=t.lower()
    for i in t:
        a=ord(i)
        if (a<=96) or (a>=123):
            T+=" "
        else:
           T+=i
    T.replace(" ","")
    return(T)

text=input()
text=clean(text)

#Goes though all of the letters in the alphabet and counts them and then adds that vaule to LFT2
for i in range(97,122):
    LFT2+=LetterCount(chr(i),text)
for i in LFT2:
    LFT+=(i/len(text)*100)

while True
